<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title></title>
</head>
<body>
    <script src="https://statics.teams.cdn.office.net/sdk/v1.6.0/js/MicrosoftTeams.min.js" integrity="sha384-mhp2E+BLMiZLe7rDIzj19WjgXJeI32NkPvrvvZBrMi5IvWup/1NUfS5xuYN5S3VT" crossorigin="anonymous"></script>
    <script type="text/javascript">
        microsoftTeams.initialize();

        // Get the tab context, and use the information to navigate to Azure AD login page
        microsoftTeams.getContext(function (context) {


            window.location.assign(`${window.location.origin}/.auth/logout?post_logout_redirect_uri=${window.location.origin}/Connector/Setup`);


        });

        microsoftTeams.authentication.notifySuccess(null, `${window.location.origin}/Connector/Setup`);
    </script>
</body>
</html>